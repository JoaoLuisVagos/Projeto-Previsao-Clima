{
    "name": "fullstack-job-frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"postcss -w ./src/styles/globals.css -o ./src/styles/tw.css\" \"next dev -p 3000\"",
        "build": "npm run build:css && next build",
        "start": "next start -p 3000",
        "build:css": "postcss ./src/styles/globals.css -o ./src/styles/tw.css --env production"
    },
    "dependencies": {
        "autoprefixer": "10.4.22",
        "framer-motion": "^12.23.24",
        "lucide-react": "^0.555.0",
        "moment": "^2.30.1",
        "next": "14.0.0",
        "postcss": "8.5.6",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "tailwindcss": "3.4.7",
        "concurrently": "7.6.0",
        "postcss-cli": "10.1.0"
    }
}